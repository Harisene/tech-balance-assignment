{
  "extends": "expo/tsconfig.base",
  "include": ["src"],
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "@resources/*": ["src/resources/*"],
      "@navigation/*": ["src/navigation/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@providers/*": ["src/providers/*"],
      "@models/*": ["src/models/*"],
      "@themes/*": ["src/themes/*"],
      "@features/*": ["src/features/*"],
      "@screens/*": ["src/screens/*"],
      "@libs/*": ["src/libs/*"],
    }
  },
}
